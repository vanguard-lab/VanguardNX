{
  "name": "@workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register -r ./load-env -P  ./libs/shared-core-lib/tsconfig.lib.json ./node_modules/typeorm/cli.js",
    "users:start": "nx run-many --parallel --target=serve --projects=users-ms",
    "users:migration:show": "npm run typeorm migration:show -- -d ./apps/users-ms/src/app/infrastructure/persistence/type-orm.config.ts",
    "users:migration:up": "npm run typeorm migration:run -- -d ./apps/users-ms/src/app/infrastructure/persistence/type-orm.config.ts --transaction=none",
    "users:migration:generate": "npm run typeorm -- -d ./apps/users-ms/src/app/infrastructure/persistence/type-orm.config.ts migration:generate ./apps/users-ms/src/app/db/migrations/migration",
    "users:migration:create": "npm run typeorm -- migration:create ./apps/users-ms/src/app/infrastructure/persistence/migrations/migration",
    "users:migration:down": "npm run typeorm -- -d ./apps/users-ms/src/app/infrastructure/persistence/type-orm.config.ts migration:revert",
    "users:schema:drop": "npm run typeorm -- -d ./apps/users-ms/src/app/infrastructure/persistence/type-orm.config.ts schema:drop"
  },
  "private": true,
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/microservices": "^11.0.20",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/typeorm": "^11.0.0",
    "@ntegral/nestjs-sentry": "^4.0.1",
    "axios": "^1.6.0",
    "body-parser": "^2.2.0",
    "bson-objectid": "^2.0.4",
    "change-case": "^5.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "string-ts": "^2.2.1",
    "typeorm": "^0.3.22",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^11.0.1",
    "@nx/eslint": "20.8.0",
    "@nx/eslint-plugin": "20.8.0",
    "@nx/js": "20.8.0",
    "@nx/nest": "20.8.0",
    "@nx/node": "20.8.0",
    "@nx/web": "20.8.0",
    "@nx/webpack": "20.8.0",
    "@nx/workspace": "20.8.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/lodash": "^4.17.16",
    "@types/node": "~18.16.20",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "nx": "20.8.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ]
}
